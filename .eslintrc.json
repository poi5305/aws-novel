{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-loop-func": "warn",
        "global-require": "warn",
        "max-len": "warn",
        "no-bitwise": "warn",
        "strict": [0, "global"],
        "no-console": "warn",
        "linebreak-style": "off",
        "comma-dangle": "warn",
        "no-restricted-syntax": "warn"
    }
}